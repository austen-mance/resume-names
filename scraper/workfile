<script type="text/javascript">
    $(function() {
       $("#emailButton3").click(function () {
            $(".jobAlerts").addClass('sticky');
            $(".jobAlerts").css("display", "block");
            $(".jobAlertButton").css("display", "none");
            $(".jobAlertButton").removeClass('nav-down');
        });

        $(".jobAlert_Close").click(function () {
            $(".jobAlerts").css("display", "none");
            $(".jobAlertButton").css("display", "block");
            $(".jobAlerts").removeClass('sticky');
        });

        /* test for nav */
    // Hide Header on on scroll down
    var didScroll;
    var lastScrollTop = 0;
    var delta = 5;
    var navbarHeight = $('.navbar-fixed-top').outerHeight();

    $(window).scroll(function (event) {
        didScroll = true;
    });

    setInterval(function () {
        if (didScroll) {
            hasScrolled();
            didScroll = false;
        }
    }, 250);

    function hasScrolled() {
        var st = $(this).scrollTop();

        // Make sure they scroll more than delta
        if (Math.abs(lastScrollTop - st) <= delta)
            return;

        // If they scrolled down and are past the navbar, add class .nav-up.
        // This is necessary so you never see what is "behind" the navbar.
        lastScrollTop = st;
        }

        if('False' == 'True')
        {
           $('#messageholder1').html("<div class=\"alert alert-success fade in\"> <strong>Success!</strong>\r\n<br>\r\nYour search agent has been saved. </div>");

        }
        
        var isLoggedin = false;
        if (isLoggedin) {
            var emailAddr = getParameterByName('addr');
            var searchQuery =  $('#searchQuery').val(); 
            if (emailAddr && searchQuery) {
                emailAction(emailAddr, searchQuery);
            }
        }

    });

    function emailAction1Click() {
        var emailaddressValue = $('#emailAddress1').val();
        var queryValue = $('#searchQuery').val();
        emailAction1(emailaddressValue, queryValue);
    }

    function emailAction1(emailaddressValue, queryValue) {
        var spinsets = { lines: 8, length: 2, width: 2, radius: 3 };
        var spinner = new Spinner(spinsets);
      
        dataLayer.push({'eventCategory':'Sidebar Widgets','eventAction':'JSA','eventLabel':'Click Link in Widget'});
        $.ajax({
            type: "POST",
            url: "http://jobs.monster.com/emailjobs",
            data: { "addr": emailaddressValue, "searchQuery": queryValue, "isDynamicPage" : $('#isDynamicPage').val()},
            beforeSend: function() {
                //$('#emailButton2').attr("disabled", true);
                $('#messageholder1').html("<div class=\"email_success\" id=\"loadingspinner\"></div>");
                var target = document.getElementById('loadingspinner');
                spinner.spin(target);
            },
            success: function(returndata) {
                if (returndata.success) {
                    if (returndata.message.IsValid) {
                     
                        if (returndata.message.IsRedirect == true) {
                            window.location.href = returndata.message.RedirectUrl;
                            return;
                        }

                        $('#messageholder1').html("<div class=\"alert alert-success fade in\">" + returndata.message.Message + "</div>");
                        $('#emailAddress1').val("");


                        //construct list of items
                        var savedSearchesArr = returndata.message.SavedSearches;
                        $('ul.savedsearcheslist').empty();
                        var cList = $('ul.savedsearcheslist');
                        $.each(savedSearchesArr, function(i) {
                            var li = $('<li/>')
                                .addClass('ui-menu-item')
                                .attr('role', 'menuitem')
                                .appendTo(cList);
                            var aaa = $('<a/>')
                                .addClass('ui-all')
                                .attr("href", savedSearchesArr[i].Href)
                                .text(savedSearchesArr[i].Name)
                                .appendTo(li);
                        });

                        
                        if (savedSearchesArr.length < 5) {
                            $('.email_button_content_logged_in').show(); 
                            $('.agent-limit').hide();                           
                        } else {
                            $('.email_button_content_logged_in').hide();
                            $('.agent-limit').show(); 
                            $('.save_search_header').hide();                           
                        }
                                   
                        var isLoggedin = false;
                        if (isLoggedin) 
                        {
                            _m.ATM.atCall("document", "o", "JSA Submit", { "events.event22": "true" });
                            $('.savedSearches_my').show();
                        } 
                        else {
                            _m.ATM.atCall("document", "o", "JSA Submit", { "events.event26": "true" });
                        } 
                        
                    } else {
                        $('#messageholder1').html("<div class=\"alert alert-warning fade in\"><div class=\"email_warning\">" + returndata.message.Message + "</div></div>");
                    }
                } else {
                    $('#messageholder1').html("<div class=\"alert alert-warning fade in\"><div class=\"email_warning\">" + returndata.message + "</div></div>");
                }
            },
            complete: function() {
                // Handle the complete event
                spinner.stop();
                //$('#emailButton2').attr("disabled", false);
            }
        });
    }

</script>
