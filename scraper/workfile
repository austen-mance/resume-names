<script type="text/javascript">
    var keywordsField = '.navbar-hover.navbar.solid-nav .keywords';
    var locationField = '.navbar-hover.navbar.solid-nav .location';
    $('#keywords').focusin(function () {
        $(keywordsField).addClass('focused');
    });
    $('#keywords').focusout(function () {
        $(keywordsField).removeClass('focused');
    });
    $('#location').focusin(function () {
        $(locationField).addClass('focused');
    });
    $('#location').focusout(function () {
        $(locationField).removeClass('focused');
    });

    var menuLinkID;
    var timeDelay = 400; //default

    $('.subnav-link').click(function () {
        menuLinkID = $(this).attr('id');
        var lastChar = menuLinkID.substr(menuLinkID.length - 1);
        menuNum(lastChar);
    });
    $('.subnav-link').hover(function () {
        menuLinkID = $(this).attr('id');
        var lastChar = menuLinkID.substr(menuLinkID.length - 1);
        timeDelay = 400; //reset default
        //set delay to nothing if menu is already opened
        resetMenu();

        //set timer to avoid accidental opening of menu
        $(this).data('timeout', setTimeout(function () {
            menuNum(lastChar);
        }, timeDelay));
    }, function () {
        clearTimeout($(this).data('timeout'));
    });

    // this is for the top right My Account link disguised as Hello, Username
    $('.loginLink2').click(function () {
        $('#subnav-link-1').addClass('activated');
        menuNum('1');
    });
    $('.loginLink2').hover(function () {
        timeDelay = 400; //reset default
        //set delay to nothing if menu is already opened
        resetMenu();

        //set timer to avoid accidental opening of menu
        $(this).data('timeout', setTimeout(function () {
            $('#subnav-link-1').addClass('activated');
            menuNum('1');
        }, timeDelay));
    }, function () {
        clearTimeout($(this).data('timeout'));
    });
    function resetMenu() {
        if ($('.navbar-2').hasClass('navbar-hover')) {
            timeDelay = 0;
        }
    }

    function menuNum(idNum) {    
        var menuNumber = idNum;
        if ($('.subnav-link').hasClass('activated')) {
            $('.subnav-sub').removeAttr('style').css({ 'transition': '0s', '-moz-transition': '0s', '-ms-transition': '0s', '-o-transition': '0s' });
            $('.subnav-link').removeClass('activated');
        }
        $('#subnav-link-' + menuNumber).removeClass('activated');
        $('#subnav-link-' + menuNumber).addClass('activated');
        $('.navbar').addClass('navbar-hover');
        $('#subnav-sub-' + menuNumber).css({ 'height': '290px' });
        $('.content, .footer').mouseover(
            function () {
                if ($('.subnav-link').hasClass('activated')) {
                    $('.navbar-form .btn-white').css({ 'transition': '0' });
                    setTimeout(function () {
                        $('.navbar-form .btn-white').removeAttr('style');
                    }, 200);
                }
                $('.subnav-sub').removeAttr('style');
                //$('#subnav-link-' + menuNumber).removeClass('activated');
                $('.subnav-link').removeClass('activated');
                $('.navbar').removeClass('navbar-hover');
                $('.navbar.solid-nav .keywords').removeClass('focused');
                $('.navbar.solid-nav .location').removeClass('focused');
                $('.navbar.solid-nav.navbar-hover .navbar-overlay').removeAttr('style');
            }
        );
    }

    $(window).scroll(function () {
        var height = $(window).scrollTop();

        $('.navbar').css({
            'box-shadow': '0 ' + (0 + (Math.min(height / 10, 2))) + 'px ' + (0 + (Math.min(height / 10, 2))) + 'px ' + 'rgba(0,0,0,.2)'
        });
        if (height > 5) {
            $('.subnav').css({
                'border-bottom-color': 'transparent'
            });
        } else {
            $('.navbar').removeAttr('style');
        }
    });

    var $sideMenu = $('.side-menu-container');
    //var $sideMenuNavHeader = $('.side-menu-container .nav-header');
    //var $contentContainer = $('.wrap .content');
    var $menulink = $('.side-menu-link');    
    var $mobileNavContainer = $('.mobile-nav-container');
    //var $mobileNavSubnavLink = $('.mobile-nav-container ul.accordion ul.submenu li a');

    $menulink.click(function () {

        var i = $(this).attr('href').slice(-1);
        var $sideMenu = $('#side-menu' + i);
        var $wrap = $('.wrap');
        
        if (($wrap.hasClass('active')) && ($(this).hasClass('active'))) {
            $menulink.removeClass('active');
            $wrap.removeClass('active');
            //$sideMenuNavHeader.removeClass('animated fadeIn');
            $('html').removeClass('side-menu-active');
            $wrap.removeClass('side-menu-active');
            $mobileNavContainer.fadeOut('slow', function () {
                $(this).removeClass('active');
            });

        }
        else if (($wrap.hasClass('active')) && (!$(this).hasClass('active'))) {
            $menulink.removeClass('active');
            $(this).addClass('active');
            $('.side-menu-container').removeClass('active');
            $sideMenu.addClass('active');
            //$sideMenuNavHeader.removeClass('animated fadeIn');
            //$sideMenuNavHeader.addClass('animated fadeIn');

        }
        else if (!$wrap.hasClass('active')) {
            $(this).addClass('active');
            $('.side-menu-container').removeClass('active');
            $mobileNavContainer.stop(true, true).addClass('active').show();
            $sideMenu.addClass('active');
            $wrap.addClass('active');
            //$sideMenuNavHeader.removeClass('animated fadeIn');
            $('html').addClass('side-menu-active');
            $wrap.addClass('side-menu-active');


        }
        return false;
    });

    // set content spacing when using jsr header
    $(window).ready(function () {
        //$('.content').css('margin-top', '50px');
    });
    
    function getJobTitleTypeAhead()
    {
       $("#keywords").typeahead(
                    {
                        hint: false,
                        highlight: true,
                        minLength: 1
                    },
                    {
                        source: function (q, cb) {
                            var arrList = [];
                            /* 132 - Job Title
                               124 - Skills Or Keyword
                            */
                            var searchType = 132;
                            var request = JSON.stringify({
                                "Query":q,
                                "MaxResults":10,
                                "SearchType":  searchType
                            });
                        
                            $.ajax({                       
                                url: "/Services/MAutoComplete.asmx/GetCompletionList?request=" + request + '',
                                type: "GET",
                                dataType: "json",
                                contentType: "application/json",
                                success: function (data) {                               
                                    if (data.d.Result.Items != null) {
                                        for (var i = 0; i < data.d.Result.Items.length && i < 10; i++) {
                                            var txt = (data.d.Result.Items[i].Text).replace(/<b>|<\/b>/g, "");
                                            arrList.push({ value: txt });
                                        }
                                        cb(arrList);
                                    }
                                }
                            });
                        }
                    }
                );
    }

    function getLocationTypeAhead(locTypeAheadUrl)
    {
        $("#location").typeahead(
                {
                    hint: false,
                    highlight: true,
                    minLength: 1
                },
                {
                    source: function (q, cb) {
                        var arrList = [];
                        $.ajax({
                            url: locTypeAheadUrl,
                            type: "POST",
                            dataType: "json",
                            contentType: "application/json",
                            data: JSON.stringify({"query":q,"countryID":'164'}),
                            success: function (data) {
                                if (data.d.Result.Items != null) {
                                    for (var i = 0; i < data.d.Result.Items.length && i < 10; i++) {
                                        var txt = (data.d.Result.Items[i].Text).replace(/<b>|<\/b>/g, "");
                                        arrList.push({ value: txt });
                                    }
                                    cb(arrList);
                                }
                            }
                        });
                    }
                }
            );
    }

    $(function () {        
                    getLocationTypeAhead('/Services/Locations.asmx/GetTrovixLocationsCompletionList');
                    getJobTitleTypeAhead();

         $("#keywords").keyup(function(event){
            if(event.keyCode == 13){
                mnstDoSearch('quickJobSearch', '0');
            }
        });

        $("#location").keyup(function(event){
            if(event.keyCode == 13){
                mnstDoSearch('quickJobSearch', '0');
            }
        });
    });

/* Sub Navigation Columnization */
    // Sub Navigation Columnization
    $(function () {
        secondComponent = $("#jsrHeader .subnav-sub-columns > .cmsNavContainer.componentDC1:nth-child(2)")
        secondComponent.each(function() {
            parenting = $(this).parent().parent().append($('<div class="col-xs-3 subnav-sub-columns"></div>'));
            parenting.find('.subnav-sub-columns').last().append($(this));
        });
        
        var num_cols = 2,
            container = $("#jsrHeader .cmsNavContainer ul"),
            listItem = 'li',
            listClass = 'sub-list';
        container.each(function() {
            var items = $(this).find(listItem);
            if (items.length > 5) {
                var items_per_col = new Array(),
                    min_items_per_col = Math.floor(items.length / num_cols),
                    difference = items.length - (min_items_per_col * num_cols);
                for (var i = 0; i < num_cols; i++) {
                    if (i < difference) {
                        items_per_col[i] = min_items_per_col + 1;
                    } else {
                        items_per_col[i] = min_items_per_col;
                    }
                }
                for (var i = 0; i < num_cols; i++) {
                    if (i == 0) {
                        for (var j = 0; j < items_per_col[i]; j++) {
                            var pointer = 0;
                            for (var k = 0; k < i; k++) {
                                pointer += items_per_col[k];
                            }
                            $(this).append(items[j + pointer]);
                        }
                    }
                    else {
                        parenting = $(this).parent().parent().parent().append($('<div class="col-xs-3 subnav-sub-columns"><div class="cmsNavContainer"></div></div>'));
                        parenting.find('.cmsNavContainer').last().append($('<h3 class="fnt5">&nbsp;</h3>')).append($('<ul></ul>').addClass(listClass));
                        for (var j = 0; j < items_per_col[i]; j++) {
                            var pointer = 0;
                            for (var k = 0; k < i; k++) {
                                pointer += items_per_col[k];
                            }
                            parenting.find('.' + listClass).last().append(items[j + pointer]);
                        }
                    }
                }
            }
        });
    });

</script>
