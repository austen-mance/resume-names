        $(document).ready(function () {
            $("#coSearch").keypress(function (e) {
                var companySearch = $("input[id$='coSearch']");
                if (e.which == 13 && companySearch.val().length > 0) {
                    triggerFilterClick(companySearch, 'company', companySearch.val());
                    monsterDoCompanySearch('quickJobSearch', '0');
                }
            });
        });

    </script>
