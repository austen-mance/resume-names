<div class="pagingWrapper">
<div class="paging light-theme simple-pagination"><ul><li class="active"><span class="current">1</span></li><li><a class="page-link" href="?q=sales&amp;where=Chicago__2C-IL&amp;page=2">2</a></li><li><a class="page-link" href="?q=sales&amp;where=Chicago__2C-IL&amp;page=3">3</a></li><li><a class="page-link" href="?q=sales&amp;where=Chicago__2C-IL&amp;page=4">4</a></li><li><a class="page-link" href="?q=sales&amp;where=Chicago__2C-IL&amp;page=5">5</a></li><li><a class="page-link" href="?q=sales&amp;where=Chicago__2C-IL&amp;page=6">6</a></li><li class="disabled"><span class="page-link">…</span></li><li><a class="page-link" href="?q=sales&amp;where=Chicago__2C-IL&amp;page=99">99</a></li><li><a class="page-link next" href="?q=sales&amp;where=Chicago__2C-IL&amp;page=2">Next</a></li></ul></div>
</div>
<script type="text/javascript">

    $(function () {

        var totalPages = 1434 >= 99 ? 99 : 1434;
        var prevText = "Previous";
        var nextText = "Next";
        var currentPage = 1;
        var edges = 1;

        var url = window.location.search;
        url = removePageParameter(url);
        var queryString = url.substring( url.indexOf('?') + 1 );
        queryString = queryString == "" ? queryString : queryString + "&";

        //For edge display
        var displayedPages = 3;
        if (currentPage <= 4) {
            displayedPages = 6;
        }
        else if (currentPage == totalPages || currentPage == totalPages - 1) {
            displayedPages = 5;
        } 
        else if (currentPage == totalPages - 2) {
            displayedPages = 4;
        }


        //For next and prev button
        if (currentPage == 1) {
            prevText = "";
        }
        if (currentPage == totalPages) {
            nextText = "";
        }
        
        $(".paging").pagination({
            displayedPages: displayedPages,
            edges: edges,
            pages: totalPages,
            currentPage: currentPage,
            hrefTextPrefix : "?" + queryString + "page=",
            prevText: prevText,
            nextText : nextText,
            selectOnClick : false,
            onInit: function (pageNumber) {
               
            },
            onPageClick : function(pageNumber, event) {
                //debugger;
                var url = event.currentTarget.href;
                if(pageNumber == 1)
                    url = removePageParameter(url); //remove page=
                var lastChar = url.slice(-1);
                if (lastChar == '?')
                    url = url.substr(0, url.length - 1);

                event.currentTarget.href = url;
            }
        });
    });

    function removePageParameter(url) {
        return url.replace(/&?((page)|(pg))=([^&]$|[^&]*)/i, ""); //remove page or pg
    }


</script>
</section>
