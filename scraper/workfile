<script type="text/javascript">

    $(function () {
        $(".input-clear").hide($(this).prev('input').val());
        $(".input-clear").click(function () {
            var t = $(this);
            t.closest('div.input-group').find('input').removeClass('input-delete-icon');
            t.closest('div.input-group').find('input').val('').focus();
            t.closest('div.input-group').find('input').typeahead('val', '');
            t.hide();
        });
        $(".hasclear").keyup(function () {
            var t = $(this);
            t.closest('div.input-group').find('.input-clear').toggle(Boolean(t.val()));
            t.addClass('input-delete-icon');
        });
    });


</script>
