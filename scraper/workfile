<script type="text/javascript">
    function triggerFilterClick(obj, type, titleValue) {

        var evar63Json = JSON.parse('{"company": [""],"category": [""],"kywdjobtitle": ["sales"],"location": ["Chicago,Illinois"],"skill": [""],"jobstatus": [""],"radius": ["20 miles"]}');
        if (titleValue === undefined || titleValue === null)
            titleValue = $(obj).children("a").attr("title");
        
        switch (type) {
            case "company":
                evar63Json.company = [];
                evar63Json.company.push(titleValue);
                break;
            case "category":
                evar63Json.category = [];
                evar63Json.category.push(titleValue);
                break;
            case "kywdjobtitle":
                evar63Json.kywdjobtitle = [];
                evar63Json.kywdjobtitle.push(titleValue);
                break;
            case "location":
                evar63Json.location = [];
                evar63Json.location.push(titleValue);
                break;
            case "skill":
                evar63Json.skill = [];
                evar63Json.skill.push(titleValue);
                break;
            case "jobstatus":
                evar63Json.jobstatus = [];
                evar63Json.jobstatus.push(titleValue);
                break;
            case "radius":
                evar63Json.radius = [];
                evar63Json.radius.push(titleValue);
                break;
            default:
                return;
        }

        var company = GetConcatStr(evar63Json.company);
        var category = GetConcatStr(evar63Json.category);
        var kywdjobtitle = GetConcatStr(evar63Json.kywdjobtitle); 
        var location = GetConcatStr(evar63Json.location);
        var skill = GetConcatStr(evar63Json.skill);
        var jobstatus = GetConcatStr(evar63Json.jobstatus);
        var radius = GetConcatStr(evar63Json.radius);
        
        var strFormat = "COMP:{0}_CAT:{1}_KW:{2}_LOC:{3}_SKILL:{4}_JOBSTATUS:{5}_RAD:{6}";
        var evar63Str = strFormat.format(company, category, kywdjobtitle, location, skill, jobstatus, radius);
        
        _m.ATM.atCall("document", "o", "Filter Click", { "eVar63": evar63Str });
    }

    function GetConcatStr(arr) {
        var joinedVal = arr.join('|');
        return joinedVal == "" ? "Unspecified" : joinedVal;
    }
    
    //for string format
    if (!String.prototype.format) {
        String.prototype.format = function () {
            var args = arguments;
            return this.replace(/{(\d+)}/g, function (match, number) {
                return typeof args[number] != 'undefined'
        ? args[number]
        : match
      ;
            });
        };
    }
</script>
</div>
